{
  "tests": "./*_test.js",
  "timeout": 10000,
  "output": "./output",
  "helpers": {
    "Puppeteer": {
      "url": "http://localhost",
      "restart": true
    }
  },
  "mocha": {
    "reporterOptions": {
      "mochaFile": "./output/result.xml"
    }
  },
  "multiple": {
    "parallel": {
      "chunks": 2
    },
    "default": {
      "grep": "signin",
      "browsers": ["chrome", "firefox"]
    }
  },
  "name": "tests",
  "bootstrap": "./bootstrap.js",
  "include": {
    "I": "./custom_steps.js",
    "Smth": "./pages/Smth.js",
    "loginPage": "./pages/Login.js",
    "signinFragment": "./fragments/Signin.js"
  }
}