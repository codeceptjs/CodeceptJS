{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "func-names": 0,
    "no-use-before-define": 0,
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "no-undef": 0,
    "prefer-destructuring": 0,
    "no-param-reassign": 0,
    "max-len": 0,
    "camelcase": 0,
    "no-shadow": 0,
    "consistent-return": 0,
    "no-console": 0,
    "global-require": 0,
    "class-methods-use-this": 0,
    "no-plusplus": 0,
    "no-return-assign": 0,
    "prefer-rest-params": 0,
    "no-useless-escape": 0,
    "no-restricted-syntax": 0,
    "one-var": 0,
    "no-unused-expressions": 0,
    "eqeqeq": 0,
    "guard-for-in": 0,
    "no-multi-assign": 0,
    "require-yield": 0,
    "prefer-spread": 0,
    "radix": 0,
    "import/no-dynamic-require": 0,
    "prefer-const": 0,
    "no-empty": 0,
    "no-continue": 0,
    "no-mixed-operators": 0,
    "default-case": 0,
    "import/no-extraneous-dependencies": 0,
    "no-bitwise": 0,
    "no-array-constructor": 0,
    "no-cond-assign": 0,
    "no-var": 0,
    "no-buffer-constructor": 0,
    "no-eval": 0,
    "array-callback-return": 0,
    "no-loop-func": 0,
    "quotes": 0,
    "import/no-unresolved": 0,
    "no-redeclare": 0,
    "function-paren-newline": 0,
    "no-useless-return": 0
  }
}
